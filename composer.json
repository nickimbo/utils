{
  "name": "nickimbo/utils",
  "description": "nickimbo utils package",
  "type": "project",
  "license": "MIT",
  "autoload": {
      "psr-4": {
            "Nickimbo\\Utils\\": "src/"
      },
      "classmap": [
        "src/Essentials.php",
        "src/Basic.php",
        "src/Path.php",
        "src/Validator/BodyValidator.php"
      ]
  },
  "authors": [
    {
      "name": "Nickimbo"
    }
  ],
  "require": {
    "php": "^8.2"
  },
  "config": {
    "bin-dir": "bin",
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
